{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run build:vercel"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://questionnaire-mock-main.vercel.app/api/$1",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "build/index.html"
    }
  ]
}