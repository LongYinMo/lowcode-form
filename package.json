  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "antd": "^5.2.0",
    "axios": "^1.3.2",
